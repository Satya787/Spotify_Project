{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifyADF03"
		},
		"DataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStorage'"
		},
		"SQL_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB'"
		},
		"DataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifydatalake03.dfs.core.windows.net/"
		},
		"SQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverspotifysatya.database.windows.net"
		},
		"SQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbsatya"
		},
		"SQL_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsatya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifyADF03_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeStorage_accountKey": {
						"value": "[parameters('DataLakeStorage_accountKey')]"
					},
					"SQL_DB_password": {
						"value": "[parameters('SQL_DB_password')]"
					},
					"DataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStorage_properties_typeProperties_url')]"
					},
					"SQL_DB_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_properties_typeProperties_server')]"
					},
					"SQL_DB_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_properties_typeProperties_database')]"
					},
					"SQL_DB_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}